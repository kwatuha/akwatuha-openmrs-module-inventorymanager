/* Extra styling for each panel*/

#panel ul {
list-style:none;
margin:0 5px;
padding:0;
}

	#panel ul li {
		padding:5px;
		color:#557482;
		border-bottom:1px dotted #ccc;
	}

	#panel ul li.last {
		border-bottom:none !important;
	}
	
	#panel ul li a {
		color:#557482;
	}

